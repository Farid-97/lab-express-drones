<h2>Available drones</h2>


{{#each drones}}
<div>
    <h3>{{name}}</h3>
    <h4 >{{propellers}}</h4>
    <h5>{{maxSpeed}}</h5>
    <a href="/drones/{{_id}}/edit">Update drones</a>
    <form action="/drones/{{id}}/delete" method="post">
    <button type="submit">
        Delete 
    </button>
    </form>
</div>

{{/each}}